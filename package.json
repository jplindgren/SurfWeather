{
  "name": "surf-weather",
  "version": "1.0.0",
  "description": "typscript node api and frontend react",
  "main": "index.js",
  "repository": "git@github.com:jplindgren/SurfWeather.git",
  "author": "Joao Paulo Lindgren <joaopozo@gmail.com>",
  "license": "MIT",
  "keywords": [
    "nodejs",
    "typescript",
    "api"
  ],
  "engines": {
    "node": "12.x"
  },
  "scripts": {
    "test": "yarn test:unit && yarn test:functional",
    "test:unit": "jest",
    "test:functional": "jest --projects ./test --runInBand",
    "build": "tsc",
    "heroku-postbuild": "yarn build",
    "start": "node dist/src/index.js",
    "start:dev": "ts-node-dev --files src/index.ts | pino-pretty -c",
    "style:check": "prettier --check 'src/**/*.ts' 'test/**/*.ts'",
    "style:fix": "prettier --write 'src/**/*.ts' 'test/**/*.ts'",
    "lint": "eslint ./src ./test --ext .ts",
    "lint:fix": "eslint ./src ./test --ext .ts --fix"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/config": "^0.0.38",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/express-pino-logger": "^4.0.2",
    "@types/express-rate-limit": "^5.1.1",
    "@types/http-status-codes": "^1.2.0",
    "@types/jest": "^26.0.20",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/lodash": "^4.14.168",
    "@types/module-alias": "^2.0.0",
    "@types/moment": "^2.13.0",
    "@types/mongoose": "^5.10.3",
    "@types/nock": "^11.1.0",
    "@types/node": "12.12.6",
    "@types/pino": "^6.3.6",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "eslint": "^7.20.0",
    "jest": "^26.6.3",
    "nock": "^13.0.9",
    "pino-pretty": "^4.6.0",
    "prettier": "^2.2.1",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.2",
    "ts-node-dev": "^1.1.2",
    "typescript": "^4.2.2"
  },
  "dependencies": {
    "@overnightjs/core": "^1.7.5",
    "axios": "^0.21.1",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "config": "^3.3.3",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-pino-logger": "^6.0.0",
    "express-rate-limit": "^5.2.6",
    "http-status-codes": "^2.1.4",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "module-alias": "^2.2.2",
    "moment": "^2.29.1",
    "mongoose": "^5.11.18",
    "pino": "^6.11.1"
  }
}
