{
  "name": "surf-weather",
  "version": "1.0.0",
  "description": "typscript node api and frontend react",
  "main": "index.js",
  "repository": "git@github.com:jplindgren/SurfWeather.git",
  "author": "Joao Paulo Lindgren <joaopozo@gmail.com>",
  "license": "MIT",
  "keywords": [
    "nodejs",
    "typescript",
    "api"
  ],
  "scripts": {
    "test": "yarn test:unit && yarn test:functional",
    "test:unit": "jest",
    "test:functional": "jest --projects ./test --runInBand",
    "build": "tsc",
    "start": "yarn build && node dist/src/index.js",
    "start:dev": "ts-node-dev 'src/index.ts'",
    "style:check": "prettier --check 'src/**/*.ts' 'test/**/*.ts'",
    "style:fix": "prettier --write 'src/**/*.ts' 'test/**/*.ts'",
    "lint": "eslint ./src ./test --ext .ts",
    "lint:fix": "eslint ./src ./test --ext .ts --fix"
  },
  "devDependencies": {
    "@types/config": "^0.0.38",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.20",
    "@types/module-alias": "^2.0.0",
    "@types/mongoose": "^5.10.3",
    "@types/nock": "^11.1.0",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "eslint": "^7.20.0",
    "jest": "^26.6.3",
    "nock": "^13.0.9",
    "prettier": "^2.2.1",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.2",
    "ts-node-dev": "^1.1.2",
    "typescript": "^4.2.2"
  },
  "dependencies": {
    "@overnightjs/core": "^1.7.5",
    "@types/node": "^14.14.31",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "config": "^3.3.3",
    "express": "^4.17.1",
    "module-alias": "^2.2.2",
    "mongoose": "^5.11.18"
  }
}
